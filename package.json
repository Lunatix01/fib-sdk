{
  "name": "fib-sdk",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Walid R. Rashed <walid@kurdi.dev>",
  "main": "dist/index.js",
  "module": "dist/fib-sdk.esm.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/fib-sdk.cjs.production.min.js",
      "limit": "30 KB"
    },
    {
      "path": "dist/fib-sdk.esm.js",
      "limit": "30 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "dts-cli": "^2.0.2",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "qs": "^6.11.1"
  }
}
